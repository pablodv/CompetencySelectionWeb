<%= render :partial => 'shared/notice' %>
<div class="app-form">
  <% form_for [@competency,@item] do |f| %>
    <%= f.error_messages %>
     <div class="field">
      <%= f.label :description %>
      <%= f.text_area :description %>
    </div>
    <div class="field">
      <%= f.label :weighted %>
      <%= f.text_field :weighted %>
    </div>
    <%= f.submit "Add" %> | <%= link_to 'Cancel', collection_path(@competency) %>
  <% end %>
</div>