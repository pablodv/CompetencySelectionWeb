<%= form.label :institute_id %><br/>
<%= form.select :institute_id, Institute.find_all_by_level(params[:level]).collect {|c| [c.name, c.id]}, {:include_blank => 'select an institute'} %><br/>
<%= form.label :specialty_id %><br/>
<%= form.select :specialty_id, Specialty.find_all_by_level(params[:level]).collect {|c| [c.name, c.id]}, {:include_blank => 'select a specialty'} %><br/>
<%= form.label :start_date %><br/>
<%= form.date_select :start_date %><br/>
<%= form.label :end_date %><br/>
<%= form.date_select :end_date %><br/>
<%= form.label :comment %><br/>
<%= form.text_area :comment %><br/>

